<div class="container">
  <h3>Sign In</h3>

  <%= form_for :session, url: session_path do |f| %>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit 'Submit' %>
  <% end %>

</div>
